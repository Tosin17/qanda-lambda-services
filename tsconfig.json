{
    "compilerOptions": {
        "preserveConstEnums": true,
        "strictNullChecks": true,
        "sourceMap": false,
        "allowJs": true,
        "target": "es2020",
        "outDir": ".build",
        "moduleResolution": "node",
        "lib": ["es2020"],
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "rootDir": "./"
    }
}
